{
  "expo": {
    "name": "ia-responde",
    "slug": "ia-responde",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "",
    "scheme": "ia-responde",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"     
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
